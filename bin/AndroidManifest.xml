<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.apc.ups_selector.app"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="14" />
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.CALL_PHONE"/>
	
    <application
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:theme="@style/seStyle">
        
        <activity
            android:name=".activity.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity android:name=".activity.Main2Activity"></activity>
        <activity android:name=".activity.SearchResultsActivity"></activity>
        <activity android:name=".activity.DetailsActivity"></activity>
        <activity android:name=".activity.ResellerMapActivity"></activity>
        
        
        <uses-library android:name="com.google.android.maps" />
    </application>

</manifest>